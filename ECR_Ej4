#include <iostream>

using namespace std;

/*
Escriba un programa en C++ que solicite el ingreso de un número entre el 1 y el 9 y muestre
la tabla de multiplicar del número ingresado.
Ejemplo:
Ingrese un numero: 9
9 x 1 = 9
9 x 2 = 18
9 x 3 = 27
9 x 4 = 36
9 x 5 = 45
9 x 6 = 54
9 x 7 = 63
9 x 8 = 72
9 x 9 = 81
9 x 10 = 90

*/


int main() {

  int num=1, mult=1, cant;

  cout<<"Ingrese un numero: "<<endl;
  cin>>cant;

  if (cant>0){
      while (num<=12){
        mult = cant * num;
    
        cout<<cant<<" x "<<num<<" = "<< mult<<endl; 
        num++;
      }    
  }
  else{
    cout<<"Ingrese un numero positivo";
  }
  

  
}

